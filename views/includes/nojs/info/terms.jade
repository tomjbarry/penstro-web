mixin NJTerms
	div#pyNJTerms.row.pyTermsGenerated
		- var key, section, i;
		span.pyTerms.col-xs-12
			div.row
				span.col-xs-12.text-center
					h1= t('terms:default.t')
				span.col-xs-1
				hr.col-xs-10
				span.col-xs-1
			div.row
				span.col-xs-12.text-center
					em.text-pad-left!= t('terms:default.p.0')
				span.col-xs-12.text-center
					strong.text-pad-left!= t('terms:default.p.1')
