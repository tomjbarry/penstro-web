mixin AdminFollowee(iter)
	+AdminOnly
		div(ng-controller='AdminFolloweeController').pyFollowee
			div.pySingle.col-xs-12.col-md-4
				span.pull-left
					+UserReference('info.username')
				span.pull-right
					+Timestamp('timeSince', 'calendar')
				div.row
					div.col-xs-12
						span.pull-left
							+SubButton(t('unfollow'), undefined, 'fa-times', 'unfollow()', undefined, undefined, undefined, undefined, undefined, 'disabled')